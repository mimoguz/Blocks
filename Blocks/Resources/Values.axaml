<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="using:System">
    <Thickness x:Key="BlockButtonPadding">24,8</Thickness>
    <Thickness x:Key="BlockImageButtonPadding">8</Thickness>
    <Thickness x:Key="BlockInputPadding">8</Thickness>
    <sys:Double x:Key="BlockIconSize">16</sys:Double>
    <sys:TimeSpan x:Key="BlockButtonStateTransitionDuration">0:0:0.075</sys:TimeSpan>
    <Thickness x:Key="BlockControlBorderThickness">2</Thickness>
    <CornerRadius x:Key="BlockButtonCornerRadius">9999</CornerRadius>
    <FontWeight x:Key="BlockButtonFontWeight">Bold</FontWeight>
    <sys:Double x:Key="BlockMinButtonSize">36</sys:Double>
    <sys:Double x:Key="BlockRadioButtonSize">28</sys:Double>
    <sys:Double x:Key="BlockRadioDotSize">16</sys:Double>
    <Thickness x:Key="BlockRadioButtonGap">8,4</Thickness>
    <CornerRadius x:Key="BlockCheckBoxCornerRadius">6</CornerRadius>
    
    <sys:Double x:Key="BlockButtonBorderOpacity">0.5</sys:Double>
    <sys:Double x:Key="BlockButtonBorderHighlightOpacity">0.9</sys:Double>
    <sys:Double x:Key="BlockButtonHighlightOpacity">0.33</sys:Double>
    <sys:Double x:Key="BlockButtonShadeOpacity">0.5</sys:Double>
    <sys:Double x:Key="BlockDisabledOpacity">0.5</sys:Double>
    <sys:Double x:Key="BlockViewBackgroundOpacity">0.2</sys:Double>
    <sys:Double x:Key="BlockViewHighlightOpacity">0.3</sys:Double>
    <sys:Double x:Key="BlockViewFocusOpacity">0.75</sys:Double>
    <sys:Double x:Key="BlockViewFocusHighlightOpacity">0.9</sys:Double>
</ResourceDictionary>
